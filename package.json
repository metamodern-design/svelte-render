{
  "name": "@metamodern/svelte-render",
  "version": "0.0.0",
  "description": "Render Svelte files for static hosting",
  "author": "Daniel C. Narey <os@metamodern.design>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/metamodern-design/svelte-render"
  },
  "type": "module",
  "main": "./dist/main.cjs",
  "module": "./dist/esm.js",
  "exports": {
    ".": "./dist/esm.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "del dist/*",
    "build": "rollup -c",
    "lint": "eslint --fix index.js src",
    "release": "np",
    "sync": "git pull && git add . && git commit -m 'automated sync' && git push",
    "test": "npm run lint && npm run build && ava",
    "update": "updtr"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.2",
    "@rollup/plugin-replace": "^2.3.1",
    "cpy": "^8.1.0",
    "fs-extra": "^9.0.0",
    "rollup": "^2.6.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "svelte": "^3.20.1"
  },
  "devDependencies": {
    "ava": "^3.5.1",
    "core-js": "^3.6.5",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-ava": "^10.2.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "eslint-plugin-types": "^1.0.2",
    "np": "^5.2.1",
    "tinydate": "^1.2.0",
    "updtr": "^3.1.0"
  },
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "tailwind",
    "tailwindcss",
    "configuration",
    "config",
    "design system",
    "design",
    "system"
  ],
  "ava": {
    "nodeArguments": [
      "--experimental-modules"
    ]
  }
}
